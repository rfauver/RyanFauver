<h1>New</h1>
<br>
<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_for(@blog) do |f| %>
      <%= f.label :title %>
      <%= f.text_field :title %>

      <%= f.label :content %>
      <%= f.text_area :content, :cols => "30", :rows => "20"%>

      <%= f.submit "Create", class: "btn btn-primary" %>
    <% end %>
  </div>
</div>
